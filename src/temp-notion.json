{
  "parent": { "database_id": "{{yourDatabaseId}}" },
  "properties": {
    "TaskName": {
      "title": [{ "text": { "content": "{{TaskName}}" } }]
    },
    "TaskContent": {
      "rich_text": [{ "text": { "content": "{{TaskContent}}" } }]
    },
    "CompleteDate": {
      "date": { "start": "{{CompleteDate}}" }
    },
    "StartDate": {
      "date": { "start": "{{StartDate}}" }
    },
    "EndDate": {
      "date": { "start": "{{EndDate}}" }
    },
    "List": {
      "select": { "name": "{{List}}" }
    },
    "Priority": {
      "select": { "name": "{{Priority}}" }
    },
    "Tag": {
      "multi_select": [{ "name": "{{Tag}}" }, { "name": "{{Tag}}" }]
    },
    "LinkToTask": {
      "url": "{{LinkToTask}}"
    },
    "CreatedAt": {
      "date": { "start": "{{CreatedAt}}" }
    }
  },
  "children": [
    {
      "object": "block",
      "type": "paragraph",
      "paragraph": {
        "rich_text": [{ "type": "text", "text": { "content": "{{TaskContentText}}" } }]
      }
    },
    {
      "object": "block",
      "type": "paragraph",
      "paragraph": {
        "rich_text": [{ "type": "text", "text": { "content": "{{TaskContentText}}" } }]
      }
    },
    {
      "object": "block",
      "type": "paragraph",
      "paragraph": {
        "rich_text": []
      }
    },
    {
      "object": "block",
      "type": "to_do",
      "to_do": {
        "rich_text": [{ "type": "text", "text": { "content": "{{TaskContentCheckList}}" } }],
        "checked": true
      }
    },
    {
      "object": "block",
      "type": "to_do",
      "to_do": {
        "rich_text": [{ "type": "text", "text": { "content": "{{TaskContentCheckList}}" } }],
        "checked": true
      }
    }
  ]
}
